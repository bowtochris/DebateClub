<?xml version="1.0" encoding="utf-8" ?>
<templates>
	<blocks>
		<block type="text" id="name">
			<location x="10" y="10"/>
			<wordwrap width="452" height="620" shrinktofit="True" />
			<text color="#000000" size="72"/>
		</block>
	</blocks>
	<template src="proxy/back.png" default="True">
		<matches>
			<match name="property" value="name" />
		</matches>
		<textblocks>
			<link block="name">
				<property name="Name"  />
			</link>
		</textblocks>
	</template>
</templates>